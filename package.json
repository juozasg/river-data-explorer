{
  "name": "river-data-explorer",
  "version": "0.5.5",
  "private": true,
  "description": "Interactive web application to visualize environmental monitoring data for St. Joseph's river",
  "license": "Apache-2.0",
  "author": "Juozas Gaigalas",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
